{
    "name": "david2m/prooph-event-store-doctrine-adapter-bundle",
    "description": "Integrate prooph/event-store-doctrine-adapter into a Symfony application.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Twomey",
            "email": "david2m@hotmail.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/http-kernel": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/framework-bundle": "^3.1",
        "prooph/event-store-symfony-bundle": "^0.2.2",
        "prooph/event-store-doctrine-adapter": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "David2M\\Bundle\\Prooph\\EventStore\\Adapter\\DoctrineAdapterBundle\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "David2M\\Bundle\\Prooph\\EventStore\\Adapter\\DoctrineAdapterBundle\\Tests\\Integration\\": "tests/integration/"
        }
    }
}
